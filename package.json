{
  "name": "gtln",
  "version": "0.0.1-rc1",
  "description": "A utility to use Graphite and Linear together",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "type": "module",
  "bin": {
    "gtln": "./dist/index.js"
  },
  "packageManager": "pnpm@8.15.5",
  "exports": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "format": "prettier .",
    "format:fix": "prettier -w .",
    "release": "dotenv release-it --"
  },
  "author": "George Gebbett",
  "license": "ISC",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/node": "^20.12.7",
    "dotenv-cli": "^7.4.2",
    "prettier": "^3.2.5",
    "release-it": "^17.2.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@linear/sdk": "^21.1.0",
    "arktype": "2.0.0-dev.12",
    "commander": "^12.0.0"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "git": {
      "commitMessage": "chore: release v${version}"
    }
  }
}
